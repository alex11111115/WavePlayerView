<?xml version="1.0" encoding="utf-8"?>

<resources>
<declare-styleable name="WaveView">
<attr name="waveColor1" format="color" />
<attr name="waveStrokeWidth1" format="dimension" />
<attr name="waveColor2" format="color" />
<attr name="waveStrokeWidth2" format="dimension" />
<attr name="waveSpeed" format="float" />
<attr name="wave2Visible" format="boolean" /> 
</declare-styleable>

<declare-styleable name="AudioVisualizerView">
<attr name="blobColor" format="color"/>
<attr name="blobStyle">
<enum name="fill" value="0"/>
<enum name="stroke" value="1"/>
<enum name="fill_and_stroke" value="2"/>
</attr>
</declare-styleable>
</resources>
